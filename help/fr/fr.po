msgid "2"
msgstr "2"

#: C/gcalctool.xml:229(para)
msgid "Octal"
msgstr "Octal"

#: C/gcalctool.xml:230(para)
msgid "8"
msgstr "8"

#: C/gcalctool.xml:233(para)
msgid "Decimal"
msgstr "Décimal"

#: C/gcalctool.xml:234(para)
msgid "10"
msgstr "10"

#: C/gcalctool.xml:237(para)
msgid "Hexadecimal"
msgstr "Hexadécimal"

#: C/gcalctool.xml:238(para) C/gcalctool.xml:2065(para)
msgid "16"

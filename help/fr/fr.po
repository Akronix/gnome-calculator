msgstr "8"

#: ../C/gcalctool.xml:231(para)
msgid "Decimal"
msgstr "Décimal"

#: ../C/gcalctool.xml:232(para)
msgid "10"
msgstr "10"

#: ../C/gcalctool.xml:235(para)
msgid "Hexadecimal"
msgstr "Hexadécimal"

